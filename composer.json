{
  "name": "arraypress/wp-register-columns",
  "version": "2.0.0",
  "description": "Lightweight library for registering custom columns in WordPress admin tables.",
  "homepage": "https://github.com/arraypress/wp-register-columns/",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\WP\\RegisterColumns\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-register-columns/issues",
    "source": "https://github.com/arraypress/wp-register-columns"
  },
  "keywords": [
    "wordpress",
    "columns",
    "admin",
    "custom-columns",
    "table-columns",
    "posts",
    "users",
    "taxonomies",
    "comments",
    "media"
  ]
}